{
  "name": "mane-loca",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron .",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build && next start & electron .",
    "package": "electron-builder --win portable"
  },
  "dependencies": {
    "@radix-ui/react-switch": "^1.1.2",
    "@tanstack/react-table": "^8.20.6",
    "@tanstack/react-table-devtools": "^8.20.6",
    "next": "14.2.22",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.4.0",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.22",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wait-on": "^8.0.1"
  },
  "build": {
    "productName": "ManeLoca",
    "appId": "com.pzweuj.maneloca.electron",
    "win": {
      "target": "portable",
      "icon": "src/app/icon.ico",
      "asar": true
    },
    "mac": {
      "icon": "src/app/icon.ico"
    },
    "linux": {
      "icon": "src/app/icon.ico"
    },
    "directories": {
      "output": "dist"
    }
  }
}
